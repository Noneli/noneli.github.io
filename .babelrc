{
  "presets": [
    "react",
    ["env", {
      "targets": {
        "browsers": ["last 3 versions", "safari >= 7"]
      }
    }]
  ],
  "plugins": [
    "transform-decorators-legacy",
    "transform-class-properties",

    ["transform-runtime", {
      "helpers": false,
      "polyfill": false,
      "regenerator": true,
      "moduleName": "babel-runtime"
    }],
    ["styled-jsx/babel", { "optimizeForSpeed": true }]
  ]
}
